SOURCE_DIR = 

TARGET_ARCH=x64Linux3gcc5.4.0

# If undefined in the environment default NDDSHOME to install dir
ifndef NDDSHOME
NDDSHOME := /home/stacy/rti_connext_dds-5.3.1
endif

JAVA_PATH = java
JAVAC_PATH = javac

JAVA_SOURCES = $(SOURCE_DIR)./Position.java $(SOURCE_DIR)./PositionSeq.java $(SOURCE_DIR)./PositionTypeSupport.java $(SOURCE_DIR)./PositionTypeCode.java $(SOURCE_DIR)./PositionDataReader.java $(SOURCE_DIR)./PositionDataWriter.java $(SOURCE_DIR)./Accident.java $(SOURCE_DIR)./AccidentSeq.java $(SOURCE_DIR)./AccidentTypeSupport.java $(SOURCE_DIR)./AccidentTypeCode.java $(SOURCE_DIR)./AccidentDataReader.java $(SOURCE_DIR)./AccidentDataWriter.java $(SOURCE_DIR)./AccidentSubscriber.java $(SOURCE_DIR)./AccidentPublisher.java
CLASS_FILES  = $(JAVA_SOURCES:%.java=%.class)

RTI_CLASSPATH := $(NDDSHOME)/lib/java/nddsjava.jar

%.class : %.java
	$(JAVAC_PATH)  -classpath ".:$(RTI_CLASSPATH)" $<

all: $(CLASS_FILES)
 
#
# Convenient way to run the java programs
#

export LD_LIBRARY_PATH := $(NDDSHOME)/lib/x64Linux3gcc5.4.0:/usr/lib/lwp:$(LD_LIBRARY_PATH)

AccidentPublisher: ./AccidentPublisher.class
	$(JAVA_PATH) -d64 -classpath ".:$(RTI_CLASSPATH)" AccidentPublisher $(ARGS)
 
AccidentSubscriber: ./AccidentSubscriber.class
	$(JAVA_PATH) -d64 -classpath ".:$(RTI_CLASSPATH)" AccidentSubscriber $(ARGS)
